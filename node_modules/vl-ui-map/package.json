    "demo": "npm run dev",
    "dev": "concurrently \"npm:bundle:watch\" \"http-server\"",
    "posttest:e2e": "pkill -f http-server",
    "prepare": "npm update --dev vl-ui-util && npm run prepare:util",
    "prepare:util": "npm explore vl-ui-util -- npm run install:copy && npm explore vl-ui-util -- npm run install:bamboo",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "npm run build",
    "release:prepare:commit": "git add -f vl-map.js && git commit --amend --no-edit && git pull",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-map/style.css' vl-map.js",
    "release:testless": "np --yolo",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "npm run test:server:start && npm run test:e2e:chrome && npm run test:e2e:firefox",
    "test:e2e:chrome": "mocha --chrome",
    "test:e2e:chrome:grid": "mocha --chrome --grid",
    "test:e2e:firefox": "mocha --firefox",
    "test:e2e:firefox:grid": "mocha --firefox --grid",
    "test:e2e:grid": "npm run test:server:start && npm run test:e2e:chrome:grid && npm run test:e2e:firefox:grid",
    "test:e2e:opera": "mocha --opera",
    "test:e2e:safari": "mocha --safari",
    "test:grid": "wct --config-file ./wct.docker.conf.json && npm run test:e2e:grid",
    "test:server:start": "http-server &",
    "test:unit": "wct --config-file ./wct.local.conf.json --verbose"
