  "scripts": {
    "explode": "node exploder.js",
    "install:bamboo": "node ./bamboo/bamboo-spec.js",
    "install:copy": "npm run install:copy:install && npm run install:copy:test && npm run install:copy:release && npm run install:copy:demo:js && npm run install:copy:demo:css && npm run install:copy:prismjs && npm run install:copy:codeowner && npm run install:copy:github && npm run install:copy:wct",
    "install:copy:codeowner": "copyfiles CODEOWNERS ../../",
    "install:copy:demo:css": "copyfiles -f demo.css ../../demo",
    "install:copy:demo:js": "copyfiles -f demo.js ../../demo",
    "install:copy:github": "cp -R .github ../../",
    "install:copy:install": "npm i copyfiles@2.1.0 --save",
    "install:copy:prismjs": "copyfiles -f 'node_modules/prismjs/**' ../../node_modules/prismjs",
    "install:copy:release": "copyfiles -f docker-compose-release.yml ../../",
    "install:copy:test": "copyfiles -f test/* ../../test",
    "install:copy:wct": "cp *.conf.json ../../",
    "install:css:minify": "minify ../../style.css > ../../style.min.css && cp ../../style.min.css ../../style.css && rm ../../style.min.css",
    "install:font:path-italic": "replace 'https://ui.vlaanderen.be/assets/fonts/italic/FlandersArtSans' 'https://cdn.milieuinfo.be/vlaanderen-font/LATEST/font/flandersart/FlandersArtSansItalic' ../../style.css",
    "install:font:path-sans": "replace 'https://ui.vlaanderen.be/assets/fonts/sans/FlandersArtSans' 'https://cdn.milieuinfo.be/vlaanderen-font/LATEST/font/flandersart/FlandersArtSans' ../../style.css",
    "install:font:path-serif": "replace 'https://ui.vlaanderen.be/assets/fonts/serif/FlandersArtSerif' 'https://cdn.milieuinfo.be/vlaanderen-font/LATEST/font/flandersart/FlandersArtSerif' ../../style.css",
    "install:font:vlaanderen-icon": "replace '/font/iconfont/vlaanderen-icon.woff' 'https://cdn.milieuinfo.be/vlaanderen-font/LATEST/font/iconfont/3.9.1/vlaanderen-icon.woff' ../../style.css",
    "install:fonts": "npm run install:font:path-sans && npm run install:font:path-serif && npm run install:font:path-italic && npm run install:font:vlaanderen-icon",
    "install:sass": "npm run install:sass:webcomponent && npm run install:sass:demo",
    "install:sass:demo": "sass demo.scss ../../demo/demo.css --load-path ../../node_modules/@govflanders",
    "install:sass:webcomponent": "sass ../../src/style.scss ../../style.css --load-path ../../node_modules/@govflanders",
    "install:utils": "npm run install:bamboo && npm run install:copy:install && npm run install:copy:codeowner && npm run install:copy:github",
    "release": "np",
    "release:prepare": "echo 2Fabiola for the bigger and bolder",
    "release:testless": "np --yolo",
    "update:readme": "bash ./scripts/updateReadme.sh",
    "util": "npm run install:sass && npm run install:copy && npm run install:css:minify && npm run install:fonts && npm run install:bamboo && npm run install:copy:github && npm run update:readme"
  },
