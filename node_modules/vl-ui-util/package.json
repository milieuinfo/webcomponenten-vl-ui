    "install:bamboo": "node ./bamboo/bamboo-spec.js",
    "install:copy": "npm run install:copy:install && npm run install:copy:test && npm run install:copy:demo:js && npm run install:copy:demo:css && npm run install:copy:prismjs && npm run install:copy:codeowner && npm run install:copy:github",
