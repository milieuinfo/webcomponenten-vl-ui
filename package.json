  },
  "scripts": {
    "install": "npm run analyze && npm run doc",
    "analyze": "npm run analyze:copy && npm run analyze:generate",
    "analyze:copy": "npm run analyze:copy:webcomponent && npm run analyze:copy:webcomponent:src && npm run analyze:copy:demo && npm run analyze:copy:style",
    "analyze:copy:webcomponent": "copyfiles -f node_modules/*/vl-*.js .",
    "analyze:copy:webcomponent:src": "copyfiles -f node_modules/*/src/vl-*.js src",
    "analyze:copy:demo": "copyfiles -f node_modules/vl-*/demo/* ./demo",
    "analyze:copy:style": "bash ./scripts/copy-style.sh",
    "analyze:generate": "polymer analyze --sources [vl-*.src.js,src/vl-*.js] > analysis.json",
    "doc": "npm run doc:generate",
    "doc:generate": "node_modules/jsdoc/jsdoc.js vl-*src.js src/vl-*.js README.md --destination doc",
    "release:prepare": "echo tjiek tsjak vollen bak",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponenten-vl-ui/issues"
  },
