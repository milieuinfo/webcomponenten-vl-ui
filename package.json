  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  },
  "scripts": {
    "install": "npm run analyze && npm run install:copy && npm run doc",
    "analyze": "npm run analyze:copy && npm run analyze:prepare && npm run analyze:generate",
    "analyze:prepare": "replace '/src/(.*).js' '../$1.js' ./demo/vl*.*",
    "analyze:copy": "npm run analyze:copy:webcomponent && npm run analyze:copy:webcomponent:src && npm run analyze:copy:demo && npm run analyze:copy:style",
    "analyze:copy:webcomponent": "copyfiles -f -e '**/*.min.js' -e '**/*.src.js' node_modules/*/dist/vl-*.js .",
    "analyze:copy:webcomponent:src": "copyfiles -f node_modules/*/src/vl-*.js src",
    "analyze:copy:demo": "npm run analyze:copy:demo:files && npm run analyze:copy:demo:directory:img",
    "analyze:copy:demo:files": "copyfiles -f node_modules/vl-*/demo/* ./demo",
    "analyze:copy:demo:directory:img": "copyfiles -f node_modules/vl-*/demo/img/* ./demo/img",
    "install:copy": "npm run install:copy:github",
    "install:copy:github": "npm explore vl-ui-util -- npm run install:copy:github",
    "analyze:copy:style": "node ./scripts/copy-styles.js",
    "analyze:generate": "polymer analyze --sources [src/vl-*.js] > analysis.json",
    "doc": "npm run doc:generate",
    "doc:generate": "node_modules/jsdoc/jsdoc.js src/vl-*.js README.md --destination doc",
    "release:prepare": "echo tjiek tsjak vollen bak",
    "release:testless": "np --yolo",
    "demo": "http-server",
    "test:e2e": "npm run test:server:start && npm run test:e2e:chrome && npm run test:e2e:firefox",
    "test:e2e:grid": "npm run test:server:start && npm run test:e2e:chrome:grid && npm run test:e2e:firefox:grid",
    "test:grid": "npm run test:e2e:grid",
    "posttest:e2e": "pkill -f http-server",
    "test:e2e:chrome": "mocha --chrome",
    "test:e2e:chrome:grid": "mocha --chrome --grid",
    "test:e2e:firefox": "mocha --firefox",
    "test:e2e:firefox:grid": "mocha --firefox --grid",
    "test:e2e:opera": "mocha --opera",
    "test:e2e:safari": "mocha --safari",
    "test:server:start": "http-server &"
  },
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponenten-vl-ui/issues"
  },
