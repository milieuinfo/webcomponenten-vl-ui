{
  "name": "webcomponenten-vl-ui",
  "version": "1.0.35",
  "description": "Webcomponenten demo",
  "main": "index.html",
  "author": "DIDM",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponenten-vl-ui.git"
  },
  "homepage": "https://github.com/milieuinfo/webcomponenten-vl-ui#readme",
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  },
  "scripts": {
    "install": "npm run analyze && npm run doc",
    "analyze": "npm run analyze:copy && npm run analyze:prepare && npm run analyze:generate",
    "analyze:prepare": "npm run analyze:prepare:src && npm run analyze:prepare:demo",
    "analyze:prepare:src": "replace 'demo/' '../demo/' ./vl-*.src.js && replace 'demo/' '../demo/' src/vl-*.js",
    "analyze:prepare:demo": "replace '.src.js' '.js' ./demo/vl*.html",
    "analyze:copy": "npm run analyze:copy:webcomponent && npm run analyze:copy:webcomponent:src && npm run analyze:copy:demo",
    "analyze:copy:webcomponent": "copyfiles -f node_modules/*/vl-*.js .",
    "analyze:copy:webcomponent:src": "copyfiles -f node_modules/*/src/vl-*.js src",
    "analyze:copy:demo": "copyfiles -f node_modules/vl-*/demo/* ./demo",
    "analyze:generate": "polymer analyze --sources [vl-*.src.js,src/vl-*.js] > analysis.json",
    "doc": "npm run doc:generate",
    "doc:generate": "node_modules/jsdoc/jsdoc.js vl-*src.js src/vl-*.js README.md --destination doc",
    "release:prepare": "echo tjiek tsjak vollen bak",
    "release:testless": "np --yolo"
  },
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponenten-vl-ui/issues"
  },
  "devDependencies": {
    "copyfiles": "2.1.0",
    "jsdoc": "^3.6.3",
    "np": "5.0.3",
    "replace": "^1.1.0"
  },
  "dependencies": {
    "prismjs": "1.16.0",
    "vl-ui-accordion": "^1.0.7",
    "vl-ui-action-group": "^1.0.12",
    "vl-ui-alert": "^1.3.4",
    "vl-ui-button": "^1.1.3",
    "vl-ui-checkbox": "^1.0.9",
    "vl-ui-core": "^1.4.4",
    "vl-ui-data-table": "^1.0.1",
    "vl-ui-form-grid": "^1.0.6",
    "vl-ui-form-message": "^2.2.1",
    "vl-ui-grid": "^1.0.6",
    "vl-ui-icon": "^2.0.6",
    "vl-ui-input-field": "^1.0.6",
    "vl-ui-input-group": "^1.0.8",
    "vl-ui-link": "^1.1.4",
    "vl-ui-link-list": "^1.0.5",
    "vl-ui-map": "^1.0.14",
    "vl-ui-modal": "^1.0.1",
    "vl-ui-pill": "^1.0.0",
    "vl-ui-properties": "^2.0.3",
    "vl-ui-select": "^1.1.6",
    "vl-ui-textarea": "^1.0.5",
    "vl-ui-titles": "^1.0.9",
    "vl-ui-toaster": "^1.0.2",
    "vl-ui-tooltip": "^1.0.7"
  },
  "bundledDependencies": [
    "prismjs",
    "vl-ui-accordion",
    "vl-ui-action-group",
    "vl-ui-alert",
    "vl-ui-button",
    "vl-ui-checkbox",
    "vl-ui-core",
    "vl-ui-data-table",
    "vl-ui-form-grid",
    "vl-ui-form-message",
    "vl-ui-grid",
    "vl-ui-icon",
    "vl-ui-input-field",
    "vl-ui-input-group",
    "vl-ui-link",
    "vl-ui-link-list",
    "vl-ui-map",
    "vl-ui-properties",
    "vl-ui-select",
    "vl-ui-textarea",
    "vl-ui-titles",
    "vl-ui-toaster",
    "vl-ui-tooltip",
    "vl-ui-modal"
  ]
}
