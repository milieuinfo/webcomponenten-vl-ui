  "scripts": {
    "generate": "npm run generate:copy && npm run generate:replace && npm run generate:doc",
    "generate:copy": "npm run generate:copy:webcomponent && npm run generate:copy:webcomponent:src && npm run generate:copy:demo && npm run generate:copy:style && npm run generate:copy:github",
    "generate:copy:demo": "npm run generate:copy:demo:files && npm run generate:copy:demo:directory:img",
    "generate:copy:demo:files": "copyfiles -f node_modules/vl-*/demo/* ./demo",
    "generate:copy:demo:directory:img": "copyfiles -f node_modules/vl-*/demo/img/* ./demo/img",
    "generate:copy:style": "node ./scripts/copy-styles.js",
    "generate:copy:webcomponent": "copyfiles -f -e '**/*.min.js' -e '**/*.src.js' node_modules/*/dist/vl-*.js .",
    "generate:copy:webcomponent:src": "copyfiles -f node_modules/*/src/vl-*.js src",
    "generate:replace": "replace '/src/(.*).js' '/$1.js' ./demo/vl*.*",
    "generate:doc": "node_modules/jsdoc/jsdoc.js --configure .jsdoc.json --verbose",
    "generate:copy:github": "npm explore vl-ui-util -- npm run install:copy:github",
    "release:prepare": "echo no release prepare necessary",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
